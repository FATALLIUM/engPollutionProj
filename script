function navigatePage(page) {
  switch (page) {
    case 0:
      window.location.href = "index.html";
      break;
    case 1:
      window.location.href = "healthRisks.html";
      break;
    case 2:
      window.location.href = "vulnerableCommunities.html";
      break;
    case 3:
      window.location.href = "solutions.html";
      break;
    default:
      window.location.href = "references.html";
      break;

  }
}

